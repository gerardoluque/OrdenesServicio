@*<!DOCTYPE html>*@
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>

    <script src="../../Scripts/jquery.tmpl.js"></script>
    @*<script src="http://ajax.microsoft.com/ajax/jquery.templates/beta1/jquery.tmpl.js" type="text/javascript"></script>*@

    <script src="@Url.Content("~/Scripts/IG/ig.ui.min.js")" type="text/javascript"></script>

    <link href="@Url.Content("~/Content/IG/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/IG/ig/jquery.ui.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/IG/base/ig.ui.css")" rel="stylesheet" type="text/css" />

</head>
<body>
        <header>
            <div id="title">
                <img src="../../Content/Image/zoe_logo_chico.png" height="70px" width="170px" style="padding-top:3px;"/>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>                        
        </header>
        
        <table border="0" cellpadding="0" cellspacing="0" style="height:85%; width:100%">
            <tr>
                <td id="menuizq">
                    @Html.Action("MostrarOpciones", "Home")
                </td>
                <td id="mainmenu">
                    @RenderBody()
                </td>
            </tr>
        </table>
</body>
</html>
