        <clear />
        <add name="AspNetSqlRoleProvider" type="System.Web.Security.SqlRoleProvider" connectionStringName="OSContext" applicationName="OrdenesServicio" />
        <add name="AspNetWindowsTokenRoleProvider" type="System.Web.Security.WindowsTokenRoleProvider" applicationName="OrdenesServicio" />
      </providers>
    </roleManager>
  </system.web>  
