    </defaultConnectionFactory>
  </entityFramework>
  <connectionStrings>
    <add name="OSContext" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=ZOEOS;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
    <!--aadd name="OSContext" connectionString="Data Source=sqlb31.webcontrolcenter.com;Initial Catalog=ZOEOS;uid=zoeitcustoms;pwd=zoe2013$;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" /-->
